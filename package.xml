<?xml version="1.0"?>
<package format="2">
  <name>forward_looking_sonar_gazebo</name>
  <version>0.0.0</version>
  <description>Forward Looking Sonar Package based on ROCKY package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
  <!-- Example:  -->
  <!-- <maintainer email="jane.doe@example.com">Jane Doe</maintainer> -->
  <maintainer email="ivan.tarifa@fieb.org.br">ivan</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
  <license>TODO</license>


  <depend>gazebo</depend>
  <depend>gazebo_plugins</depend>
  <depend>cv_bridge</depend>

  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>opencv3</exec_depend>

  <test_depend>rosunit</test_depend>
  <test_depend>rostest</test_depend>
  
  <build_depend>opencv3</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>gazebo_dev</build_depend>

  <buildtool_depend>catkin</buildtool_depend>


  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <gazebo_ros gazebo_media_path="${prefix}"/>
  </export>
</package>
